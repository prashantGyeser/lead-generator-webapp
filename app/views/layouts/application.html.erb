          </div>

        </div>
      </div> <!--/ .head-cta -->

      <div class="col-md-12 product-teaser">

        <div class="row">

          <div class="col-md-6 col-sm-6 col-md-offset-3 col-sm-offset-3" id="tour-2">
            <!--<img class="center-image img-responsive" src="img/ss.png" alt="product-teaser" />-->
            <%= image_tag "header-image.png", :class => "center-image img-responsive" %>
            </a>
