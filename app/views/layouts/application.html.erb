<!-- Start pricing box -->
<section id="pricing" class="contain colorbg">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="divider clearfix"></div>
        <h4 class="heading wow flipInX" data-wow-delay="0.2s"><span>Pricing</span></h4>
        <div class="pricing-wrapper">
          <i class="fa fa-table icon-title wow rotateIn" data-wow-delay="0.4s"></i>
          <div class="row">
            <div class="col-md-4 col-md-offset-4">
              <div class="pricing-box centered wow bounceInDown" data-wow-delay="0.6s">
                <div class="pricing-head">
                  <h4>Pro</h4>
                  <div class="pricing-price">
                    <span class="price">$99</span>
                    <span class="time">/month</span>
                  </div>
                </div>
                <div class="pricing-body">
                  <ul>
                    <li>Free update</li>
                    <li>128 Mbps Download speed</li>
                    <li>1 year services</li>
                    <li>No business aplication</li>
                    <li>Support on IOS</li>
                    <li class="bottom"><a href="#" class="btn btn-default btn-lg">Get it now</a></li>
                  </ul>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- End pricing box -->

<!-- Start contact -->
<section id="contact" class="contain colorbg">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="divider clearfix"></div>
        <h4 class="heading wow flipInX" data-wow-delay="0.2s"><span>Contact us</span></h4>
      </div>
    </div>
    <div class="row">
      <div class="col-md-8 col-md-offset-2">
        <div class="contact-wrapper">
          <i class="fa fa-envelope icon-title wow rotateIn" data-wow-delay="0.4s"></i>
          <div class="contact-body wow rotateInDownLeft" data-wow-delay="0.4s">
            <p>
              <strong>Address :</strong> Jl.Kemacetan terus No.3 Block.R2 Jakarata - Indonesia<br />
              <strong>Phone :</strong> +6221 999 999 22 - <strong>Email :</strong> info@yourdomain.com
            </p>
            <form id="contactform" action="contact/contact.php" method="post" class="validateform" name="leaveContact">
              <div class="clearfix"></div>
              <div id="sendmessage">
                <div class="alert alert-info marginbot35">
                  <button type="button" class="close" data-dismiss="alert">&times;</button>
                  <strong>Your message has been sent. Thank you!</strong><br />
                </div>
              </div>
              <ul class="listForm cbp-so-section">
                <li class="first-list cbp-so-side cbp-so-side-left">
                  <input class="form-control input-name input-lg" type="text" name="name" data-rule="maxlen:4" data-msg="Required field" placeholder="Enter your full name . . ." />
                  <div class="validation"></div>
                </li>
                <li class="cbp-so-side cbp-so-side-right">
                  <input class="form-control input-email input-lg" type="text" name="email" data-rule="email" data-msg="Please enter a valid email" placeholder="Enter your email address . . ."/>
                  <div class="validation"></div>
                </li>
                <li class="full-list cbp-so-side cbp-so-side-bottom">
                  <textarea class="form-control input-message input-lg" rows="9" name="message" data-rule="required" data-msg="Please write something for us" placeholder="Write something for us . . ."></textarea>
                  <div class="validation"></div>
                </li>
                <li class="full-list text-center cbp-so-side cbp-so-side-bottom">
                  <input type="submit" value="Send message" class="btn btn-default btn-lg" name="Submit" />
                </li>
              </ul>
            </form>
          </div>
        </div>
        <div class="divider pull-left"></div>
      </div>
    </div>
  </div>
</section>
<!-- End contact -->

